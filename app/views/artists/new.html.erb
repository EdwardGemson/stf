<%= form_for @artist do |f|%>
<fieldset>
  <legend> Create an Artist Account</legend>
  <div>
    <%= f.label :email%>
    <%= f.text_field :email%>
  </div>
  <div>
    <%= f.label :password%>
    <%= f.password_field :password%>
  </div>
  <div>
    <%= f.label :password_confirmation%>
    <%= f.password_field :password_confirmation%>
  </div>
  <div>
    <%=f.submit 'Create Profile'%>
  </div>
<%end%>
